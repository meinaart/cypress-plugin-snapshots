{
  "baseUrl": "http://localhost:8080",
  "ignoreTestFiles": [
    "**/__snapshots__/*",
    "**/__image_snapshots__/*"
  ],
  "env": {
    "cypress-plugin-snapshots": {
      "serverPort": 2222,
      "diffLines": 4,
      "excludeFields": ["ignore"]
    }
  },
  "video": false,
  "viewportWidth": 660,
  "viewportHeight": 1000
}
